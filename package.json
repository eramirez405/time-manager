{
  "name": "twiliotimemanager",
  "version": "1.0.0",
  "description": "Time manager for twilio flex",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "git-update": "git pull && pm2 restart twiliotimemanager",
    "deploy": "pm2 start npm --name 'twiliotimemanager' -- run 'dev'"
  },
  "keywords": [
    "twilio",
    "flex",
    "node",
    "reactjs"
  ],
  "author": "Erasmo Ramirez",
  "license": "ISC",
  "dependencies": {
    "@googleapis/docs": "^0.2.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "config": "^3.3.2",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "date-fns": "^2.21.1",
    "download": "^8.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "googleapis": "^92.0.0",
    "hbs": "^4.1.2",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "libphonenumber-js": "^1.9.11",
    "moment": "^2.29.1",
    "mongoose": "^5.10.13",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.6.2",
    "notie": "^4.3.1",
    "npm": "^7.10.0",
    "puppeteer": "^10.0.0",
    "react-datepicker": "^4.2.1",
    "react-icons": "^4.2.0",
    "react-loader-spinner": "^4.0.0",
    "react-switch": "^6.0.0",
    "request-ip": "^2.1.3",
    "socket.io": "^3.0.5",
    "sweetalert2": "^11.3.3",
    "twilio": "^3.72.0",
    "validate-phone-number-node-js": "0.0.1",
    "winston": "^3.3.3",
    "winston-syslog": "^2.4.4",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
